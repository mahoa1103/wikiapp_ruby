<%= form_with model: test do |form| %>
    <div>
      <%= form.label :title %><br>
      <%= form.text_field :title %> <br>
      <% test.errors.full_messages_for(:title).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
  
    <div>
      <%= form.label :description %><br>
      <%= form.text_area :description %><br>
      <% test.errors.full_messages_for(:description).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>

    <div>
      <%= form.label :type %><br>
      <%= form.text_area :typeof %><br>
      <% test.errors.full_messages_for(:typeof).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
  
    <div>
      <%= form.submit %>
    </div>
  <% end %>